{
  "name": "decorators",
  "version": "0.0.1",
  "description": "CSS constructor for your React components",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js -o css-constructor.js",
    "build:example": "browserify example/entry.js -o example/bundle.js",
    "start:example": "list example/",
    "dev": "run-p build build:example start:example"
  },
  "keywords": "react, css, constructor",
  "author": "siddharthkp",
  "license": "MIT",
  "dependencies": {
      "babel-plugin-transform-decorators-legacy": "1.3.4"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "list": "0.9.0",
    "npm-run-all": "3.1.2",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      "transform-es2015-modules-commonjs"
    ]
  }
}
